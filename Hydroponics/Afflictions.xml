  <Affliction name="Low Dopamine" identifier="cokelow" description="You are experiencing depression, sadness, low neural and motor functions." type="poison" causeofdeathdescription="Died of Cocaine overdose" selfcauseofdeathdescription="You have died of Cocaine overdose." limbspecific="false" indicatorlimb="Torso" showiconthreshold="50" maxstrength="100">
