<?xml version="1.0" encoding="utf-8"?>
<Afflictions>
  <!-- Thinking on addiction and withdrawal -->
  <!-- Nicotine Withdrawal should stun the user very briefly every couple of milestones -->
  <!-- as a gameplay hinderance to not smoking and perhaps including psychosis to  -->
  <!-- make players not want to quit -->
  <Affliction name="Marijuana Paralysis" identifier="mjparalysis" type="resistance" description="Your vision is getting dull, why is my mouth so dry?" causeofdeathdescription="You had too much weed." selfcauseofdeathdescription="Earth was nice, You smoked more weed than physically possible by real humans..." limbspecific="false" indicatorlimb="Head" activationthreshold="20" maxstrength="241">
    <!-- Slow the player down and paralyze in the end-->
    <Effect minstrength="0" maxstrength="20" strengthchange="-0.4" minscreenblur="0.2" maxscreenblur="0.4">
      <StatusEffect target="Character" setvalue="true" SpeedMultiplier="0.8" />
    </Effect>
    <Effect minstrength="20" maxstrength="60" strengthchange="-0.4" minscreenblur="0.3" maxscreenblur="0.5">
      <StatusEffect target="Character" setvalue="true" SpeedMultiplier="0.7" />
    </Effect>
    <Effect minstrength="60" maxstrength="100" strengthchange="-0.4" minscreenblur="0.4" maxscreenblur="0.6">
      <StatusEffect target="Character" setvalue="true" SpeedMultiplier="0.6" />
    </Effect>
    <Effect minstrength="100" maxstrength="140" strengthchange="-0.4" minscreenblur="0.5" maxscreenblur="0.7" minscreendistort="0.1" maxscreendistort="0.3">
      <StatusEffect target="Character" setvalue="true" SpeedMultiplier="0.5" />
    </Effect>
    <Effect minstrength="140" maxstrength="160" strengthchange="-0.4" minscreenblur="0.6" maxscreenblur="0.8" minscreendistort="0.2" maxscreendistort="0.5">
      <StatusEffect target="Character" setvalue="true" SpeedMultiplier="0.4" />
    </Effect>
    <Effect minstrength="160" maxstrength="180" strengthchange="-0.4" minscreenblur="0.7" maxscreenblur="0.9" minscreendistort="0.3" maxscreendistort="0.6">
      <StatusEffect target="Character" setvalue="true" SpeedMultiplier="0.3" />
    </Effect>
    <Effect minstrength="180" maxstrength="200" strengthchange="-0.4" minscreenblur="0.8" maxscreenblur="1.0" minscreendistort="0.6" maxscreendistort="0.8">
      <StatusEffect target="Character" setvalue="true" SpeedMultiplier="0.2" />
    </Effect>
    <Effect minstrength="200" maxstrength="220" strengthchange="-0.4" minscreenblur="0.8" maxscreenblur="1.0" minscreendistort="0.8" maxscreendistort="1.0">
      <StatusEffect target="Character" setvalue="true" SpeedMultiplier="0.15" />
    </Effect>
    <Effect minstrength="220" maxstrength="240" strengthchange="-0.4" minscreenblur="0.8" maxscreenblur="1.0" minscreendistort="0.8" maxscreendistort="1.0">
      <StatusEffect target="Character" setvalue="true" SpeedMultiplier="0.1" />
    </Effect>
    <Effect minstrength="240" maxstrength="241" strengthchange="-1" minscreenblur="0.8" maxscreenblur="1.0" minscreendistort="0.8" maxscreendistort="1.0">
      <StatusEffect target="Character" setvalue="true">
        <Affliction identifier="stun" amount="100" />
        <ReduceAffliction identifier="damage" amount="5" />
        <ReduceAffliction identifier="opiatewithdrawal" amount="5" />
        <ReduceAffliction identifier="opiateaddiction" amount="5" />
      </StatusEffect>
      <StatusEffect target="Character" setvalue="true" SpeedMultiplier="0" />
    </Effect>
    <icon texture="%ModDir%/Malborasek with Sound/Textures/marijuannaicondebuff.png" sourcerect="0,0,128,128" color="195,136,60,255" origin="0,0" />
  </Affliction>
  <Affliction name="Nicotine withdrawal" identifier="nicotinewithdrawal" description="Headache, agitation, anxiety and signs of nausea." type="poison" causeofdeathdescription="Died of nicotine withdrawal. I blame Neurotrauma!" selfcauseofdeathdescription="You have died of nicotine withdrawal." limbspecific="false" indicatorlimb="Torso" showiconthreshold="20" maxstrength="100">
    <!-- General scale of withdrawal -->
    <!-- 0-20: Nothing -->
    <!-- 20-40: 0-20 health decrease and speed of 0.8 -->
    <!-- Stuns should occur between 40-100 -->
    <!-- 40-80: 20-40 health decrease and speed of 0.7 -->
    <!-- 80-100:40-50 health decrease and speed of 0.5 -->
    <!-- Slow down the character and slightly decrease vitality -->
    <Effect minstrength="20" maxstrength="40" minvitalitydecrease="0" maxvitalitydecrease="20" strengthchange="-0.05" minchromaticaberration="0.0" maxchromaticaberration="1.0">
      <StatusEffect target="Character" SpeedMultiplier="0.8" setvalue="true" />
    </Effect>
    <!-- Decrease vitality more. The strength of the affliction drops faster, meaning that 
    the character needs a stronger nicotine addiction for the withdrawal symptoms to stay at this level -->
    <Effect minstrength="40" maxstrength="80" minvitalitydecrease="20" maxvitalitydecrease="40" strengthchange="-0.1" minchromaticaberration="0.0" maxchromaticaberration="1.0">
      <StatusEffect target="Character" SpeedMultiplier="0.7" setvalue="true" />
    </Effect>
    <!-- Withdrawal symptoms start to get annoying at this level -->
    <!-- minscreendistort="0.0" -->
    <!-- maxscreendistort="1.0" -->
    <!-- minradialdistort="0.0" -->
    <!-- maxradialdistort="5.0" -->
    <Effect minstrength="80" maxstrength="100" minvitalitydecrease="40" maxvitalitydecrease="50" strengthchange="-0.1" minchromaticaberration="3" maxchromaticaberration="5">
      <StatusEffect target="Character" SpeedMultiplier="0.5" color="195,136,60,255" setvalue="true" />
    </Effect>
    <icon texture="%ModDir%/Hydroponics/Icons/nicotinewithdrawalicon120px.png" sourcerect="0,0,128,128" color="209,121,84,255" origin="0,0" />
  </Affliction>
  <Affliction name="Nicotine overdose" identifier="nicotineoverdose" description="Pale skin, off-balance, slow and shallow breathing." type="poison" causeofdeathdescription="Died of nicotine overdose. Bruh moment." selfcauseofdeathdescription="You have died of nicotine overdose." limbspecific="false" indicatorlimb="Torso" showiconthreshold="20" karmachangeonapplied="-1" maxstrength="100">
    <Effect minstrength="0" maxstrength="5" strengthchange="-0.1"></Effect>
    <!-- No effects except slight distortion at this level -->
    <Effect minstrength="5" maxstrength="50" strengthchange="-0.1" minchromaticaberration="0.0" maxchromaticaberration="1.0"></Effect>
    <!-- Starts to become lethal when strength is above 50 -->
    <Effect minstrength="50" maxstrength="100" minvitalitydecrease="0" maxvitalitydecrease="180" minscreendistort="0.0" maxscreendistort="3.0" strengthchange="-0.1" minchromaticaberration="0.0" maxchromaticaberration="10.0" minradialdistort="0.0" maxradialdistort="10.0">
      <StatusEffect target="Character" SpeedMultiplier="0.5" setvalue="true" />
    </Effect>
    <icon texture="%ModDir%/Hydroponics/Icons/nicotineodicon120px.png" sourcerect="0,0,128,128" color="209,121,84,255" origin="0,0" />
  </Affliction>
  <!-- Gradually applies the "nicotine withdrawal" affliction to the character -->
  <!-- Addiction itself should not impact players when they smoke within the first tier. -->
  <!-- Tiers should be counted per normal cigarette, more than one cig in a time should -->
  <!-- always lead to maximum addiction tier. -->
  <Affliction name="Nicotine addiction" identifier="nicotineaddiction" description="Stressed?!... Need... a smoke. Now!" type="resistance" limbspecific="false" indicatorlimb="Head" showiconthreshold="35" maxstrength="100" karmachangeonapplied="-0.01">
    <Effect minstrength="0" maxstrength="35" strengthchange="-0.02" />
    <Effect minstrength="35" maxstrength="50" strengthchange="-0.02">
      <StatusEffect target="Character">
        <Affliction identifier="nicotinewithdrawal" strength="0.15" />
      </StatusEffect>
    </Effect>
    <Effect minstrength="50" maxstrength="80" strengthchange="-0.02">
      <StatusEffect target="Character">
        <Affliction identifier="nicotinewithdrawal" strength="0.15" />
      </StatusEffect>
    </Effect>
    <Effect minstrength="80" maxstrength="100" strengthchange="-0.01">
      <StatusEffect target="Character">
        <Affliction identifier="nicotinewithdrawal" strength="0.3" />
      </StatusEffect>
    </Effect>
    <icon texture="%ModDir%/Hydroponics/Icons/nicotineicon120px.png" sourcerect="0,0,128,128" color="195,180,60,255" origin="0,0" />
  </Affliction>
  <!-- Coffee High -->
  <Affliction name="Caffeinated" identifier="cfHigh" description="Gotta go FAST!" type="resistance" isbuff="true" causeofdeathdescription="You had too much coffee." selfcauseofdeathdescription="Died from Coffee overdose... How the fuck did you do that?" limbspecific="false" indicatorlimb="Torso" activationthreshold="20" maxstrength="121">
