  <Affliction name="Caffeine overdose" identifier="coffeeoverdose" description="Confusion, fast heartbeat, slow and shallow breathing." type="poison" causeofdeathdescription="Died of Caffeine overdose" selfcauseofdeathdescription="You have died of Caffeine overdose." limbspecific="false" indicatorlimb="Torso" showiconthreshold="20" karmachangeonapplied="-1" maxstrength="100">
