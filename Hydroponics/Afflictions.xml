        <ReduceAffliction identifier="drunk" amount="5" />
      </StatusEffect>
    </Effect>
    <icon texture="%ModDir%/Hydroponics/Icons/coffeeicon120px.png" sourcerect="0,0,128,128" color="10,193,114,255" origin="0,0" />
  </Affliction>
  <Affliction name="Caffeine withdrawal" identifier="coffeewithdrawal" description="Headache, agitation, anxiety and signs of nausea." type="poison" causeofdeathdescription="Died of Caffeine withdrawal. Certified Neurotrauma moment." selfcauseofdeathdescription="You have died of Caffeine withdrawal. Blame neurotrauma." limbspecific="false" indicatorlimb="Torso" showiconthreshold="20" maxstrength="100">
